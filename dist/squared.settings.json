{
  "apiVersion": "1.0.0",
  "disk_read": false,
  "disk_write": false,
  "unc_read": false,
  "unc_write": false,
  "env": "development",
  "port": {
    "production": "80",
    "development": "3000"
  },
  "routing": {
    "common": [
      { "mount": "html", "path": "/" },
      { "mount": "dist", "path": "/dist" }
    ],
    "production": [],
    "development": [
      { "mount": "build", "path": "/build" },
      { "mount": "tmp", "path": "/tmp" }
    ]
  },
  "cors": {
    "origin": false
  },
  "request_post_limit": "250mb",
  "compress": {
    "gzip_level": 9,
    "brotli_quality": 11,
    "chunk_size": 0,
    "tinify_api_key": "",
    "7za_bin": ""
  },
  "task": {
    "gulp": {
      "handler": "@squared-functions/task/gulp",
      "settings": {}
    }
  },
  "document": {
    "android": {
      "handler": ""
    },
    "chrome": {
      "handler": "@squared-functions/document/chrome",
      "eval_function": true,
      "eval_template": false,
      "settings": {
        "html": {
          "posthtml": {
            "transform": {
              "plugins": []
            },
            "transform-output": {}
          },
          "prettier": {
            "beautify": {
              "parser": "html",
              "printWidth": 120,
              "tabWidth": 4
            }
          },
          "html-minifier-terser": {
            "minify": {
              "collapseWhitespace": true,
              "collapseBooleanAttributes": true,
              "removeEmptyAttributes": true,
              "removeRedundantAttributes": true,
              "removeScriptTypeAttributes": true,
              "removeStyleLinkTypeAttributes": true,
              "removeComments": true
            }
          }
        },
        "css": {
          "postcss": {
            "transform": {
              "plugins": []
            }
          },
          "prettier": {
            "beautify": {
              "parser": "css",
              "tabWidth": 4
            }
          },
          "clean-css": {
            "minify": {
              "inline": ["none"],
              "level": 1
            }
          }
        },
        "js": {
          "@babel/core": {
            "es5": {
              "presets": ["@babel/preset-env"]
            }
          },
          "prettier": {
            "beautify": {
              "parser": "babel",
              "printWidth": 120,
              "tabWidth": 4
            }
          },
          "terser": {
            "minify": {
              "toplevel": false,
              "sourceMap": false
            }
          },
          "uglify-js": {
            "es5-minify": {
              "toplevel": false
            }
          },
          "rollup": {
            "bundle": {
              "treeshake": false,
              "external": [],
              "output": {
                "sourcemap": false
              }
            }
          }
        },
        "view": {
          "ejs": {
            "name": "ejs",
            "compile": {
              "rmWhitespace": false
            },
            "output": {}
          }
        }
      }
    }
  },
  "image": {
    "handler": "@squared-functions/image/jimp",
    "webp": ""
  },
  "watch": {
    "interval": 200
  },
  "cloud": {
    "cache": {
      "aws": 0,
      "azure": 0,
      "gcloud": 0,
      "ibm": 0,
      "oci": 0
    },
    "aws": {
      "main": {
        "accessKeyId": "",
        "secretAccessKey": "",
        "region": ""
      },
      "db-main": {
        "accessKeyId": "",
        "secretAccessKey": "",
        "region": "",
        "endpoint": ""
      }
    },
    "azure": {
      "main": {
        "accountName": "",
        "accountKey": ""
      },
      "db-main": {
        "endpoint": "",
        "key": ""
      }
    },
    "gcloud": {
      "main": {
        "keyFilename": ""
      },
      "db-main": {
        "keyFilename": ""
      }
    },
    "ibm": {
      "main": {
        "apiKeyId": "",
        "serviceInstanceId": "",
        "endpoint": "",
        "region": ""
      },
      "db-main": {
        "account": "",
        "password": "",
        "url": ""
      }
    },
    "oci": {
      "main": {
        "namespace": "",
        "region": "",
        "accessKeyId": "",
        "secretAccessKey": ""
      },
      "db-main": {
        "user": "",
        "password": "",
        "connectionString": ""
      }
    }
  },
  "logger": {
    "unknown": true,
    "system": true,
    "node": true,
    "process": true,
    "compress": true,
    "watch": true,
    "file": true,
    "cloud": true,
    "time_elapsed": true
  }
}