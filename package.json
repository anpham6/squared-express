{
  "name": "squared-express",
  "version": "0.5.2",
  "description": "Express server for use with squared.",
  "main": "dist/serve.js",
  "scripts": {
    "build": "rimraf build/* && eslint ./src/**/*.ts && tsc && rollup --silent -c rollup.config.js",
    "debug": "tsc --inlineSourceMap -outDir ./debug",
    "serve": "tsc --outDir ./debug && cd ./debug && node serve.js --access-all",
    "dev": "tsc && copyfiles -a ./build/* -u 1 ../squared",
    "prod": "npm run clean && npm run build",
    "deploy": "copyfiles -a ./dist/* -u 1 ../squared",
    "clean": "rimraf build/* && rimraf tmp/* && rimraf debug/tmp/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anpham6/squared-express.git"
  },
  "keywords": [
    "squared",
    "squared-functions",
    "express",
    "compression"
  ],
  "author": "An Pham <anpham6@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/anpham6/squared-express#readme",
  "dependencies": {
    "@squared-functions/file-manager": "^0.13.3",
    "archiver": "^5.2.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "decompress": "^4.2.1",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "js-yaml": "^4.0.0",
    "mime-types": "^2.1.29",
    "request": "^2.88.2",
    "toml": "^3.0.0",
    "uuid": "^8.3.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@azure/cosmos": "^3.9.5",
    "@azure/storage-blob": "^12.4.1",
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@cloudant/cloudant": "^4.3.0",
    "@google-cloud/bigquery": "^5.5.0",
    "@google-cloud/firestore": "^4.9.4",
    "@google-cloud/storage": "^5.8.0",
    "@types/archiver": "^5.1.0",
    "@types/cors": "^2.8.10",
    "@types/decompress": "^4.2.3",
    "@types/express": "^4.17.11",
    "@types/fs-extra": "^9.0.7",
    "@types/js-yaml": "^4.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.14.31",
    "@types/node-7z": "^2.1.0",
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "aws-sdk": "^2.850.0",
    "clean-css": "^5.1.0",
    "copyfiles": "^2.4.1",
    "csv-parse": "^4.15.3",
    "cwebp-bin": "^6.1.1",
    "dwebp-bin": "^1.0.0",
    "eslint": "^7.20.0",
    "file-type": "^16.2.0",
    "gulp": "^4.0.2",
    "ibm-cos-sdk": "^1.9.0",
    "node-7z": "^2.1.2",
    "oracledb": "^5.1.0",
    "postcss": "^8.2.6",
    "posthtml": "^0.15.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
