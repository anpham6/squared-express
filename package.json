{
  "name": "squared-express",
  "version": "0.2.0",
  "description": "Express server for use with squared.",
  "main": "dist/serve.js",
  "scripts": {
    "build": "rimraf build/* && eslint ./src/**/*.ts && tsc && rollup --silent -c rollup.config.js",
    "debug": "tsc --inlineSourceMap -outDir ./debug",
    "serve": "tsc --outDir ./debug && cd ./debug && node serve.js --access-all",
    "dev": "tsc && copyfiles -a ./build/* -u 1 ../squared",
    "prod": "npm run build && copyfiles -a ./dist/* -u 1 ../squared"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anpham6/squared-express.git"
  },
  "keywords": [
    "squared",
    "squared-functions",
    "express",
    "compression"
  ],
  "author": "An Pham <anpham6@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/anpham6/squared-express#readme",
  "dependencies": {
    "@squared-functions/file-manager": "^0.9.10",
    "7zip-bin": "^5.0.3",
    "archiver": "^5.1.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "js-yaml": "^4.0.0",
    "node-7z": "^2.1.2",
    "request": "^2.88.2",
    "uuid": "^8.3.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@azure/cosmos": "^3.9.3",
    "@azure/storage-blob": "^12.3.0",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@cloudant/cloudant": "^4.3.0",
    "@google-cloud/bigquery": "^5.5.0",
    "@google-cloud/firestore": "^4.8.1",
    "@google-cloud/storage": "^5.7.0",
    "@types/archiver": "^5.1.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.6",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.20",
    "@types/node-7z": "^2.1.0",
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "aws-sdk": "^2.821.0",
    "clean-css": "^4.2.3",
    "copyfiles": "^2.4.1",
    "csv-parse": "^4.14.2",
    "cwebp-bin": "^6.1.1",
    "dwebp-bin": "^1.0.0",
    "eslint": "^7.17.0",
    "gulp": "^4.0.2",
    "ibm-cos-sdk": "^1.9.0",
    "imagemin-pngquant": "^9.0.1",
    "oracledb": "^5.1.0",
    "postcss": "^8.2.2",
    "posthtml": "^0.15.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "terser": "^5.5.1",
    "ts-node": "^9.1.1",
